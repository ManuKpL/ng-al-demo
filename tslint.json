{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "quotemark": [
            true,
            "single"
        ],
        "object-literal-sort-keys": false,
        "object-literal-key-quotes": false,
        "interface-name": [
            true,
            "never-prefix"
        ],
        "arrow-parens": false
    },
    "rulesDirectory": []
}
